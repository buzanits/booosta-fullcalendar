            "type": "composer",
            "url": "https://packagist.org",
            "exclude": ["npm-asset/fullcalendar"]
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0.0",
        "booosta/calendar": "^4.0",
        "npm-asset/fullcalendar": "^5.10"
    }
